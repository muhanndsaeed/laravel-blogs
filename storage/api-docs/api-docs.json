    "servers": [
        {
            "url": "http://127.0.0.1:8000/api",
            "description": "Demo API Server"
        }
    ],
    "paths": {
        "/register": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "as a user i can register by email and password",
                "operationId": "register",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "user_name",
                                    "email",
                                    "phone_number",
                                    "password",
                                    "confirm_password"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Noura"
                                    },
                                    "user_name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Noura19"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Noura@example.com"
                                    },
                                    "phone_number": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "0555555555"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Garage1234"
                                    },
                                    "confirm_password": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Garage1234"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User successfully registered"
                    },
                    "404": {
                        "description": "validator error"
                    }
                }
            }
        },
        "/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "as a user i can login by email or username and password",
                "operationId": "login",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "password"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Noura"
                                    },
                                    "user_name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Noura19"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Garage1234"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login successfully"
                    },
                    "401": {
                        "description": "invalid credentials"
                    }
                }
            }
        },
        "/logout": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "as a user i can log out",
                "operationId": "logout",
                "parameters": [
                    {
                        "name": "bearerAuth",
                        "in": "header",
                        "description": "Bearer {access-token}",
                        "required": true,
                        "schema": {
                            "type": "String"
                        }
                    },
                    {
                        "name": "Accept",
                        "in": "header",
                        "description": "application/json",
                        "required": true,
                        "schema": {
                            "type": "String"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Logged out"
                    }
                }
            }
        },
        "user/updatePassword": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "as a user i can change my password",
                "operationId": "updatePassword",
                "parameters": [
                    {
                        "name": "bearerAuth",
                        "in": "header",
                        "description": "Bearer {access-token}",
                        "required": true,
                        "schema": {
                            "type": "String"
                        }
                    },
                    {
                        "name": "Accept",
                        "in": "header",
                        "description": "application/json",
                        "required": true,
                        "schema": {
                            "type": "String"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "password",
                                    "new_password",
                                    "new_password_confirmation"
                                ],
                                "properties": {
                                    "password": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Garage1234"
                                    },
                                    "new_password": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Garage12345"
                                    },
                                    "new_password_confirmation": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Garage12345"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Your current password is change"
                    },
                    "401": {
                        "description": "Your current password is incorrect"
                    },
                    "402": {
                        "description": "Some error happened , please try again"
                    }
                }
            }
        },
        "user/profile/edit": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "as a user i can update on my profile",
                "operationId": "updateProfile",
                "parameters": [
                    {
                        "name": "bearerAuth",
                        "in": "header",
                        "description": "Bearer {access-token}",
                        "required": true,
                        "schema": {
                            "type": "String"
                        }
                    },
                    {
                        "name": "Accept",
                        "in": "header",
                        "description": "application/json",
                        "required": true,
                        "schema": {
                            "type": "String"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Garage"
                                    },
                                    "phone_number": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "0555555555"
                                    },
                                    "user_name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Garage23"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Garage@gmail.com"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Update successfully"
                    },
                    "402": {
                        "description": "Some error happened , please try again"
                    }
                }
            }
        },
        "user/profile": {
            "get": {
                "tags": [
                    "Authentication"
                ],
                "summary": "as a user i can show my profile",
                "operationId": "profile",
                "responses": {
                    "200": {
                        "description": "USER"
                    },
                    "404": {
                        "description": "No such Found!"
                    }
                }
            }
        }
    },
